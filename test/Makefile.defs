ROOT_DIR = $(shell dirname $(realpath $(lastword $(MAKEFILE_LIST))))
CALICO_VERSION ?= v3.24.0
CLUSTER_POD_SUBNET_V4 := 10.244.0.0/16
CLUSTER_POD_SUBNET_V6 := fd00:10:244::/56
CLUSTER_SERVICE_SUBNET_V4 := 10.96.0.0/16
CLUSTER_SERVICE_SUBNET_V6 := fd00:10:96::/112
E2E_KUBECONFIG := ${ROOT_DIR}/../.tmp/config/.kube/config
IP_FAMILY ?= dual
DEFAULT_CNI ?= k8s-pod-network
