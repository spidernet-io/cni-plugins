# Multus
MULTUS_FIRST_VLAN=0
MULTUS_SECOND_VLAN=100

VLAN_GATEWAY_CONTAINER="vlan-gateway"

E2E_TIMEOUT ?= 60m

CNI_PLUGINS_VERSION=v1.1.1

# proxy
RUN_ON_LOCAL=false

